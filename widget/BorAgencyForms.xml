	<form name="FindContractors" type="single" target="findcontractors" default-entity-name="Shipment">

		<field name="noConditionFind">
			<hidden value="Y" /><!-- if this isn't there then with all fields empty no query will be done -->
		</field>
		<field name="name" title="${uiLabelMap.Name}">

			<text size="10" maxlength="20" />
		</field>

		<field name="searchButton" title="${uiLabelMap.CommonFind}" widget-style="smallSubmit">
			<submit button-type="button" />
		</field>


	</form>


	<form name="ListContractors" type="list" list-name="listIt" paginate-target="findcontractors" odd-row-style="alternate-row"
		default-table-style="basic-table hover-bar" header-row-style="header-row-2" override-list-size="true">
		<actions>
			<set field="requestParameters.noConditionFind" value="Y" />
			<set field="entityName" value="BorContractor" />
			<service service-name="performFind" result-map="result" result-map-list="listIt">
				<field-map field-name="inputFields" from-field="requestParameters" />
				<field-map field-name="entityName" from-field="entityName" />
				<field-map field-name="orderBy" from-field="sortField" />
				<field-map field-name="viewIndex" from-field="viewIndex" />
				<field-map field-name="viewSize" from-field="viewSize" />
			</service>
		</actions>

		<field name="contractorId" title="${uiLabelMap.GeneralId}" widget-style="buttontext" sort-field="true">
			<hyperlink description="${contractorId}" target="EditContractor" also-hidden="false">
				<parameter param-name="contractorId" />
			</hyperlink>
		</field>

		<field name="name" title="${uiLabelMap.Name}" widget-style="buttontext" sort-field="true">
			<display />


		</field>


		<field name="deleteLink" title=" " widget-style="buttontext">
			<hyperlink description="${uiLabelMap.CommonDelete}" target="deleteContractor" also-hidden="false"
				confirmation-message="${uiLabelMap.CommonConfirmDelete}" image-location="${iconsLocation}/delete.png">
				<parameter param-name="contractorId" />
			</hyperlink>
		</field>


	</form>
	<form name="EditContractor" type="single" target="updateContractor" title="" default-map-name="contractor"
		default-entity-name="BorContractor">
		<alt-target use-when="contractor==null" target="createContractor" />
		<!-- <auto-fields-service service-name="updateShipment"/> -->
		<field use-when="contractor!=null" name="contractorId" title="${uiLabelMap.GeneralId}">
			<display />
		</field>
		<field use-when="contractor==null @and contractorId==null" name="contractorId" title="${uiLabelMap.GeneralId}">
			<ignored />
		</field>


		<field name="name">
			<text size="10" maxlength="20" />
		</field>


		<field name="submitButton" use-when="contractor==null" title="${uiLabelMap.CommonCreate}">
			<submit button-type="button" />
		</field>
		<field name="submitButton" use-when="contractor!=null" title="${uiLabelMap.CommonUpdate}">
			<submit button-type="button" />
		</field>
		<!-- <field name="deleteLink" use-when="shipping!=null" title=" " widget-style="buttontext"> <hyperlink description="${uiLabelMap.CommonDelete}" 
			target="deleteShipping" also-hidden="false" confirmation-message="${uiLabelMap.CommonConfirmDelete}" image-location="${iconsLocation}/delete.png"> 
			<parameter param-name="shipmentId"/> </hyperlink> </field> -->


	</form>



