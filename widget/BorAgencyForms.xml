	<form name="FilterGeneral" type="single" target="findConditions">
		<alt-target use-when="priceCheckPage=='Y'" target="findPriceChecks" />
		<alt-target use-when="promotionPage=='Y'" target="findPromotions" />
		<alt-target use-when="clientnotePage=='Y'" target="EditClientNote" />
		<alt-target use-when="contractPage=='Y'" target="findContracts" />
		<alt-target use-when="reportPage=='Y'" target="findReports" />
		<field name="filcontractorId" title="${uiLabelMap.ContractorId}">
			<lookup target-form-name="LookupContractor" />
		</field>
		<field use-when="clientnotePage!='Y' @and contractPage!='Y'" name="filclientId" title="${uiLabelMap.ClientId}">
			<lookup target-form-name="LookupClient" />
		</field>
		<field use-when="clientnotePage!='Y' @and contractPage!='Y'" name="filproductId" title="${uiLabelMap.ProductId}">
			<lookup target-form-name="LookupProduct?contractorId=${filcontractorId}" />
		</field>
		<field use-when="clientnotePage=='Y'" name="clientId">
			<hidden />
		</field>

		<field use-when="clientnotePage!='Y' @and priceCheckPage!='Y'" name="filactiv" title="${uiLabelMap.Activ}">
			<drop-down allow-empty="true">
				<option key="Y" description="${uiLabelMap.CommonY}" />
				<option key="N" description="${uiLabelMap.CommonN}" />
			</drop-down>
		</field>

		<field use-when="clientnotePage=='Y' || reportPage=='Y'" name="filclientnoteFrom">
			<date-time/>
